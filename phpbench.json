{
  "runner.path": "benchmark",
  "runner.bootstrap": "vendor/autoload.php",
  "runner.output_mode": "time",
  "runner.time_unit": "milliseconds",
  "runner.file_pattern": "*",
  "runner.php_config": {
    "opcache.enable": true,
    "opcache.enable_cli": true,
    "opcache.jit": 1235,
    "apc.enable": true,
    "apc.enable_cli": true
  },
  "report.generators": {
    "default": {
      "extends": "aggregate",
      "title": "PHP Routers Benchmark",
      "cols": [
        "benchmark",
        "subject",
        "mem_peak",
        "mode",
        "worst",
        "mean",
        "best",
        "stdev",
        "rstdev"
      ]
    }
  },
  "report.outputs": {
    "sort": {
      "column": "mean"
    }
  }
}
